{
  "name": "grid-monorepo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "start": "cd frontend && npm run start",
    "backend": "python -m uvicorn core_engine.api_gateway:app --reload --port 8001",
    "grid": "concurrently \"npm:backend\" \"npm:dev\"",
    "health-check": "powershell -File scripts/health-check.ps1"
  },
  "workspaces": [
    "frontend"
  ],
  "devDependencies": {
    "concurrently": "^9.2.1"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.68.0"
  }
}
